  "include": [".expo/types/**/*.ts", "expo-env.d.ts", "nativewind-env.d.ts"],
  "exclude": [],
  "references": [
    {
      "path": "./test"
    },
    {
      "path": "./tsconfig.src.json"
    },
    {
      "path": "../lib"
    }
  ]
